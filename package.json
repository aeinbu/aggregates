{
  "name": "@aeinbu/aggregates",
  "version": "0.0.0",
  "description": "Lightweight and non-intrusive aggregate functions to use with array reducers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test-watch": "jest --config jestconfig.json --watch",
    "build": "tsc",
    "prepare": "npm run build",
    "version": "git add package.json",
    "lint-autofix": "eslint --fix src/**/* tests/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeinbu/aggregates.git"
  },
  "keywords": [
    "minof",
    "min of",
    "maxof",
    "max of",
    "sumof",
    "sum of"
  ],
  "author": "Arjan Einbu <arjan@einbu.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aeinbu/aggregates/issues"
  },
  "homepage": "https://github.com/aeinbu/aggregates#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3"
  }
}
