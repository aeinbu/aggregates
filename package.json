{
  "name": "@aeinbu/aggregates",
  "version": "0.0.0",
  "description": "Lightweight and non-intrusive aggregate functions to use with array reducers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*", "src/**/*"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "test-watch": "jest --config jestconfig.json --watch",
    "build": "tsc",
    "prepare": "npm run build",
    "version": "git add package.json",
    "lint-autofix": "eslint --fix src/**/* tests/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aeinbu/groupby.git"
  },
  "keywords": [
    "minof", "min of",
    "maxof", "max of",
    "sumof", "sum of"
  ],
  "author": "Arjan Einbu <arjan@einbu.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aeinbu/aggregates/issues"
  },
  "homepage": "https://github.com/aeinbu/aggregates#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
